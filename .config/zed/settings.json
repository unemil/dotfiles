{
  "theme": "Ros√© Pine",
  "features": {
    "inline_completion_provider": "copilot"
  },
  "buffer_font_family": "Menlo",
  "buffer_font_features": {
    "calt": false
  },
  "buffer_font_size": 13,
  "ui_font_family": "Menlo",
  "ui_font_size": 14,
  "vim_mode": true,
  "restore_on_startup": "none",
  "cursor_blink": false,
  "current_line_highlight": "none",
  "redact_private_values": true,
  "show_copilot_suggestions": false,
  "show_whitespaces": "none",
  "calls": {
    "share_on_join": false
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "scrollbar": {
    "symbols_selections": false
  },
  "gutter": {
    "code_actions": false,
    "folds": false
  },
  "indent_guides": {
    "enabled": false
  },
  "seed_search_query_from_cursor": "selection",
  "project_panel": {
    "file_icons": false,
    "folder_icons": false,
    "indent_size": 10,
    "auto_fold_dirs": true
  },
  "collaboration_panel": {
    "button": false
  },
  "chat_panel": {
    "button": false
  },
  "message_editor": {
    "auto_replace_emoji_shortcode": false
  },
  "notification_panel": {
    "button": false
  },
  "assistant": {
    "version": "2",
    "enabled": false
  },
  "show_call_status_icon": false,
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "preview_tabs": {
    "enabled": false
  },
  "extend_comment_on_newline": false,
  "soft_wrap": "editor_width",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "auto_update": false,
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "terminal": {
    "toolbar": {
      "title": false
    },
    "blinking": "off",
    "line_height": "standard"
  },
  "languages": {
    "CSV": {
      "show_inline_completions": false
    },
    "YAML": {
      "format_on_save": "off"
    }
  },
  "lsp": {
    "gopls": {
      "initialization_options": {
        "usePlaceholders": false
      }
    }
  },
  "task": {
    "show_status_indicator": false
  }
}
